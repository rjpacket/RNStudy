<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.rjp.shell.ui.activity.SaveNewsActivity"
    >
    <com.rjp.shell.views.CommonTitleBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
    </com.rjp.shell.views.CommonTitleBar>

    <com.flyco.tablayout.CommonTabLayout
        android:id="@+id/tab_layout"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:background="#ffffff"
        app:tl_iconVisible="false"
        app:tl_indicator_color="@color/main_color"
        app:tl_textSelectColor="@color/main_color"
        app:tl_textUnselectColor="#66000000"
        app:tl_underline_color="#DDDDDD"
        app:tl_underline_height="1dp"
        app:tl_indicator_anim_enable="false"
        />

    <FrameLayout
        android:id="@+id/league_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

    </FrameLayout>
</LinearLayout>
