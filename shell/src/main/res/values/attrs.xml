<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension"/>
        <attr name="border_inside_color" format="color"/>
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>

    <declare-styleable name="SettingView">
        <attr name="setting_icon" format="reference" />
        <attr name="setting_icon_visible" format="boolean" />
        <attr name="setting_name" format="string" />
        <attr name="setting_name_size" format="integer" />
        <attr name="setting_name_color" format="reference|color" />
        <attr name="setting_desc" format="string" />
        <attr name="setting_desc_size" format="integer" />
        <attr name="setting_desc_color" format="reference|color" />
    </declare-styleable>
</resources>